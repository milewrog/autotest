<p id="notice"><%= notice %></p>

<h1>Listing Test Results</h1>

<table>
  <thead>
    <tr>
      <th>System under test</th>
      <th>Passed</th>
      <th>Run by whom</th>
      <th>Test details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_results.each do |test_result| %>
      <tr>
        <td><%= test_result.system_under_test %></td>
        <td><%= test_result.passed %></td>
        <td><%= test_result.run_by_whom %></td>
        <td><%= test_result.test_details %></td>
        <td><%= link_to 'Show', test_result %></td>
        <td><%= link_to 'Edit', edit_test_result_path(test_result) %></td>
        <td><%= link_to 'Destroy', test_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test result', new_test_result_path %>
